		<script>
  function addtocart(prId) {
    console.log("hi");
    $.ajax({
        url: "/addtocart",
        method: "post",
        data: { product_id: prId },
        success: (response) => {
            if (response.success) {
                const popup = Swal.fire({
                    position: "center",
                    icon: "success",
                    title: "Added to Cart",
                    showConfirmButton: false,
                    timer: 1400 // Set timer to 2 seconds
                });

                // Close the pop-up after 2 seconds
                setTimeout(() => {
                    popup.close();
                }, 2000);
            }
        },
    });
}










		</script>

	

