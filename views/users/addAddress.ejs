  <section   class="ftco-section contact-section bg-light">
    <div style="margin-left: 460px; "  class="container">
      <div   class="row block-9">
        <div class="col-md-12 order-md-last d-flex">
          <form action="/addAddress" class="bg-white p-5 contact-form" method="post">
            <div class="form-group">
              <input type="text" class="form-control" name="userName" id="input1" placeholder="Your Name" required>
              <p id="para1" style="color: red;"></p>
            </div>
            <div class="form-group">
              <input type="number" class="form-control" name="mobile" id="input2" placeholder="Your mobile" required>
              <p id="para2" style="color: red;"></p>
            </div>
            <div class="form-group">
              <input type="number" class="form-control" name="alterMobile" id="input3" placeholder="Your Alternative number" required>
              <p id="para3" style="color: red;"></p>
            </div>
            <div class="form-group">
              <textarea name="address" id="input4" class="col-md-12" rows="3" placeholder="Enter Address"></textarea>
              <p id="para4" style="color: red;"></p>
            </div>
            <div class="form-group">
              <input type="text" id="input5" name="city" placeholder="Your city" class="form-control" required>
              <p id="para5" style="color: red;"></p>
            </div>
            <div class="form-group">
              <input type="text" id="input6" name="state" placeholder="Your state" class="form-control" required>
              <p id="para6" style="color: red;"></p>
            </div>
            <div class="form-group">
              <input type="number" id="input7" name="pincode" placeholder="Your pincode" class="form-control" required>
              <p id="para7" style="color: red;"></p>
            </div>
            <div class="form-group">
              <input type="text" id="input8" name="landmark" placeholder="Your landmark" class="form-control" required>              
              <p id="para8" style="color: red;"></p>
            </div>
            <div class="form-group text-center">
              <input type="submit" onclick="show()" value="Submit" class="btn btn-primary py-3 px-5">
            </div>
            <div class="text-center">
              <%
            if(typeof message != 'undefined'){
              %>
              <p style="color: green;"><%=message%></p>
              <%
            }
            %>
            </div>
          </form>
        
        </div>
      </div>
